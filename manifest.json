{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["https://betclic.pt/mybets"],
      "js": ["jquery-3.1.0.slim.min.js", "popup_2.js"]
    }
  ],
  "web_accessible_resources": ["jquery-3.1.0.slim.min.js", "popup_2.js"],

  "permissions": [
    "activeTab",
    "https://betclic.pt/mybets"
  ]
}